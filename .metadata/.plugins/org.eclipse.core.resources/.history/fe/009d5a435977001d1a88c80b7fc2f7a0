package java_q;

import java.util.ArrayList;
import java.util.List;

public class Main {

	public static void main(String[] args) {
		List<Father> abc = new ArrayList<>();
		abc.add(new Son("cde", 1, "ddd"));
		Son son = (Son) abc;

		if (abc instanceof Son) {
			System.out.println(son.c + "변환완료");
		}
	}
}