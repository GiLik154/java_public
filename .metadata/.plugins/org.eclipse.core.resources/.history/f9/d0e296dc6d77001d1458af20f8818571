package baejoon;

public class b_1330 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 10;
		int b = 2;
		
		if(a>b) {
			System.out.println(">");
		}else if (a<b) {
			System.out.println("<");
		}else if (a==b) {
			System.out.println("==");
		}
		
		
	}

}
