        if (1 <= n && n <= 16) {
            for (int i = 0; i < n; i++) {
                y = (int) Math.pow(2, n) - 1;
                System.out.print("abc");
                System.out.print(arr1);
                System.out.print(arr2);
            }
        } else {
            System.out.println("1~16사이 숫자로 다시 입력하십시오.");
        }
    }
